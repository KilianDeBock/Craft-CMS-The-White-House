<!DOCTYPE html>
<html lang="en">
<head>
    {% include "partials/_meta" %}

    <link rel="stylesheet" href="{{ mix('app.css') }}">
    <title>{{ title ?? 'The White House' }}</title>
</head>
<body>
{% set sites = craft.app.sites.getAllSites() %}
{% set translations = {
    navigation: staticTranslations.translationsNavigation.one(),
    languages: staticTranslations.translationsLanguages.one(),
    connectForm: staticTranslations.translationsConnectForm.one(),
} %}
{% include "components/_header" %}

<main class="main">
    {% block content %}{% endblock %}
</main>

{% include "components/_footer" %}

{% block bottomScripts %}
    {% include "partials/_scripts" %}
{% endblock %}
</body>
</html>
