{% set segments = craft.app.request.getSegments() %}
<ul>
    {% for segment in segments %}
        {% if not loop.last %}
            {% set page = craft.entries.slug(segment).one() %}
            <li>
                <a href="{{ page.url }}">
                    {{ page.title }}
                </a>
            </li>
        {% endif %}
    {% endfor %}
</ul>